<h1>Listing susus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @susus.each do |susu| %>
      <ul>
        <li><%= susu.name %></li>
        <li><%= susu.total_value %></li>
        <li><%= susu.created_at %></li>
      </ul>
      <tr>
        <td><%= link_to 'Show', susu %></td>
        <td><%= link_to 'Edit', edit_susu_path(susu) %></td>
        <td><%= link_to 'Destroy', susu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Susu', new_susu_path %>
